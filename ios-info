regex_hostname = re.compile(r'(\S+\d+)\s\S+\suptime')
regex_uptime = re.compile(r'\S+\suptime\sis\s(.+)')
regex_version = re.compile(r'Cisco\sIOS\sSoftware.+Version\s([^,]+)')
regex_serial = re.compile(r'System\sSerial\sNumber\s+:\s+(\S+)')
regex_ios = re.compile(r'System\simage\sfile\sis\s"([^ "]+)')
regex_model = re.compile(r'Model\sNumber\s+:\s+(\S+)')
regex_memory = re.search(r'\(.*\)\s(.*)\sbytes\sof\smemory', sh_ver_output)
